{
    "parser": "google",
    "include_paths": [ "../../**/*.py" ],
    "exclude_paths": [
        "**/*_stub.py",
        "**/service.py",
        "**/web_service_test.py",
        "../../venv/**/*.py",
        "**/final_project/*.py",
        "**/final_project/**/*.py",
        "**/Library/**/*.py",
        "**/.*/**/*.py",
        "**/site-packages/**/*.py"
    ],
    "fail_on_missing_docstring": true,
    "fail_on_missing_summary": true,
    "fail_on_raises_section": false,
    "exclude_functions": [
        "init_application"
    ]
}
